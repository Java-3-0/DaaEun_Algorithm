import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();

		starFunc(N);

		sc.close();
	}

	private static void starFunc(int N) {

		for (int r = 0; r < N; r++) {
			for (int c = 0; c < N; c++) {
				if(r/3 == 1 & c/3 ==1) {
					starFunc(N/3);
				} else if (r == N / 3 & c == N / 3) {
					System.out.print(" ");
				} else {
				System.out.print("*");
				}
			}
			System.out.println();
		}
	}
}
