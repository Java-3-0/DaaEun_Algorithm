import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();

		starFunc(N);

		sc.close();
	}

	private static void starFunc(int N) {

		for (int r = 0; r < N; r++) {
			for (int c = 0; c < N; c++) {
				if(r== N/3&c==N/3) System.out.print(" ");
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
